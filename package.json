{
  "name": "package-status",
  "version": "1.0.0",
  "description": "Toggle packages through the status bar",
  "main": "./lib/package-status",
  "engines": {
    "atom": ">=1.13.0"
  },
  "repository": "https://github.com/eadwu/package-status",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.1.0": "consumeStatusBar"
      }
    }
  },
  "configSchema": {
    "packageList": {
      "type": "array",
      "order": 1,
      "title": "Package List",
      "default": [],
      "items": {
        "type": "string"
      }
    }
  }
}
