{
  "name": "package-status",
  "version": "1.0.0",
  "description": "Toggle packages through the status bar",
  "main": "./lib/package-status",
  "engines": {
    "atom": ">=1.13.0"
  },
  "repository": "https://gitlab.com/eadwu/package-status",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-plugin-flowtype": "^2.35.0",
    "standard": "^10.0.3"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.1.0": "consumeStatusBar"
      }
    }
  },
  "configSchema": {
    "packageList": {
      "type": "array",
      "order": 1,
      "title": "Package List",
      "default": [],
      "items": {
        "type": "string"
      }
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "atom"
    ],
    "env": {
      "browser": true,
      "node": true
    }
  }
}
